                                                   GridPane.columnIndex="1" GridPane.rowIndex="7">
                                            <HBox.margin>
                                                <Insets right="5.0"/>
                                            </HBox.margin>
                                            <GridPane.margin>
                                                <Insets left="5.0" right="5.0"/>
                                            </GridPane.margin>
                                        </TextField>

                                        <Label text="XML文件包全名" GridPane.rowIndex="8"/>
                                        <TextField fx:id="mapperTargetPackage" prefHeight="27.0" prefWidth="152.0"
                                                   promptText="mybatis.mappers.auto" text="mybatis.mappers.auto"
                                                   GridPane.columnIndex="1"
                                                   GridPane.rowIndex="8">
                                            <HBox.margin>
                                                <Insets right="5.0"/>
                                            </HBox.margin>
                                            <GridPane.margin>
                                                <Insets left="5.0" right="5.0"/>
                                            </GridPane.margin>
                                        </TextField>

                                        <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="200.0"
                                              spacing="40.0" GridPane.columnIndex="1" GridPane.columnSpan="3"
                                              GridPane.rowIndex="9">
                                            <children>
                                                <Button mnemonicParsing="false" onAction="#generateCode"
                                                        text="生成代码">
                                                    <styleClass>
                                                        <String fx:value="btn-success"/>
                                                        <String fx:value="btn"/>
                                                    </styleClass>
                                                </Button>
                                                <Button mnemonicParsing="false" onAction="#saveGenerateCodeTemplate"
                                                        text="保存配置">
                                                    <styleClass>
                                                        <String fx:value="btn-success"/>
                                                        <String fx:value="btn"/>
                                                    </styleClass>
                                                </Button>
                                            </children>
                                        </HBox>
                                    </children>
                                </GridPane>
                            </children>
                            <padding>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </padding>
                        </VBox>
                    </children>
